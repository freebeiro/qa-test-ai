{
  "manifest_version": 3,
  "name": "QA Testing Assistant",
  "version": "1.0",
  "description": "AI-powered QA Testing Assistant",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "windows",
    "tabCapture",
    "system.display"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "QA Testing Assistant"
  },
  "background": {
    "service_worker": "background.js"
  }
}